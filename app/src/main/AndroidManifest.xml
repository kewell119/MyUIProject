<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kewell.study.myuiproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".spinner.SpinnerActivity"  android:label="@string/app_name"/>
        <activity android:name=".seekbar.SeekbarActivity" android:label="@string/app_name"/>
        <activity android:name=".gridview.GridViewActivity" android:label="@string/app_name"/>
        <activity android:name=".listview.ListViewActivity" android:label="@string/app_name"/>
        <activity android:name=".listview.ListViewActivity2" android:label="@string/app_name"/>
        <activity android:name=".tabhost.TabHostActivity" android:label="@string/app_name"/>
        <activity android:name=".tabhost.TabHostGameActivity" android:label="@string/app_name"/>
        <activity android:name=".tabhost.TabHostMusicActivity" android:label="@string/app_name"/>
        <activity android:name=".tabhost.TabHostSportsActivity" android:label="@string/app_name"/>
    </application>

</manifest>
